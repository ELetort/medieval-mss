                     <supportDesc material="perg">
                        <support>parchment</support>
                        <extent>1 (medieval parchment) + 5 (modern paper)
                           leaves.
                           
                           
                        <dimensions unit="mm" type="leaf">
                              <height>340</height>
                              <width>235</width>
                           </dimensions>
