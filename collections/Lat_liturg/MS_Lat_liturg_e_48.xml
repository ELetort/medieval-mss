                  <msItem xml:id="MS_Lat_liturg_e_48-item1" class="#liturgica #biblia">
						               <title type="desc" key="work_14114">Psalter</title>
                     <textLang mainLang="la">Latin</textLang>
                     <msItem xml:id="MS_Lat_liturg_e_48-item1-item1">
                        <locus>(pp. 1-5)</locus>
                        
                        <rubric>Incipiunt dicta sancti
                           Ieromini que sunt virtutes psalmorum</rubric>
                        
                        <incipit>Canticum psalmorum animam decorat.
                           fuga est demonum</incipit>
                        <explicit>vel compunctionis graciam infundat</explicit>
                        <note>(cf. Stegmüller,
                           nos. 7211, 8542)</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_e_48-item1-item2">
