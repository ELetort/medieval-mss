                        <locus>(fol.3–6, 8–123, 127–8)</locus>
                        <note>Temporale</note>
                        <note>(beginning missing)</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_c_37-item1-item2">
                        <locus>(fol.128v, 135, 124–5)</locus>
                        <note>Office for the dedication</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_c_37-item1-item3">
                        <locus>(fol.125v, 126, 129, 133)</locus>
                        <note>Office for the dead</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_c_37-item1-item4">
                        <locus>(fol.133, 131, 132, 130)</locus>
                        <note>Venite</note>
                     </msItem>
                     <msItem xml:id="MS_Lat_liturg_c_37-item1-item5">
                        <locus>(fol.130v, 2, 7, 134, 136–151v)</locus>
