                        <author key="person_217200604">
                           William of Pagula
                        </author>
                        <title>Summa summarum</title>
                        <note>Book
                           III.39 </note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                  </msContents>
                  <physDesc>
                     
                     <objectDesc form="sheet">
                        <supportDesc material="perg">
                           <support>Parchment</support>
                           <extent>
                              <dimensions type="leaf" unit="mm">
                                 <height>330</height>
                                 <width>215</width>
                              </dimensions>
                              <dimensions type="ruled" unit="mm">
                                 <height>285</height>
                                 <width>165</width>
                              </dimensions>
                           </extent>
                          
                        </supportDesc>
                        <layoutDesc>
                           <layout columns="2" writtenLines="68">Ruled apparently in very faint leadpoint, with two columns of 68 lines</layout>
                        </layoutDesc>
                     </objectDesc>
                     <handDesc>
                        <handNote>Written in gothic script..</handNote>
                     </handDesc>
                     <decoDesc>
                        <decoNote type="rubrication">Headings in red ink; capitals stroked in red; paraphs and underlinings in red.</decoNote>
                        <decoNote type="other">One
                           two-line initial in blue, with red penwork.</decoNote>
                     </decoDesc>
                     
                  </physDesc>
                  <history>
                     <origin>
                        <origPlace>
                           <country key="place_7002445">English</country>
                        </origPlace>
                        <origDate calendar="Gregorian" notBefore="1300" notAfter="1400">14th century</origDate>
                     </origin>
                     <provenance>
                        <persName role="formerOwner" key="person_56996723">
                           F. S. Ferguson
                        </persName>; given by him to <persName role="formerOwner" key="person_100180276">
