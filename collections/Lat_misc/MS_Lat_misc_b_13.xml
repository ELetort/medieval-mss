               <msPart xml:id="MS_Lat_misc_b_13-part6">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno type="part">MS. Lat. misc. b. 13, fols. 55-61</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <msItem xml:id="MS_Lat_misc_b_13-part6-item1">
                        <title>Legal text</title>
                        <note>(unidentified; includes a section from Book IV)</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <objectDesc form="fragment">
                        <supportDesc material="perg">
                           <support>Parchment</support>
                           <extent>Seven cuttings.
                              <dimensions type="leaf" unit="mm">
                                 <height>110</height>
                                 <width>110</width>
                              </dimensions>
                              <note>(dimensions of each cutting; each column c. 85 mm. wide, the inter-columnar space c. 15 mm. wide)</note>
                              <dimensions type="ruled" unit="mm">
                                 <height/>
                                 <width/>
                              </dimensions>
                           </extent>
                         
                        </supportDesc>
                        <layoutDesc>
                           <layout columns="2" writtenLines="60">Ruled in brown 'crayon', in two columns; each cutting with up to 29 lines, so a full leaf would probably have had at least 60 lines per column</layout>
                        </layoutDesc>
                     </objectDesc>
                     <handDesc>
                        <handNote>Written in a neat rounded gothic hand.</handNote>
                     </handDesc>
                     <decoDesc>
                        <decoNote type="rubrication">One heading in red; paraphs in red or blue, running titles in red and blue; some captials stoked in a orange-yellow wash.</decoNote>
                        <decoNote type="other">Part of a three-line 'puzzle'
                           initial survives, in red and blue with red and mauve penwork; a two-line intial
                           is in red with simple mauve penwork.</decoNote>
                     </decoDesc>
                    
                  </physDesc>
                  <history>
