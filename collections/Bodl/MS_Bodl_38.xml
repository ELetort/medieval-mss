                     <msItem xml:id="MS_Bodl_38-part1-item1" n="1">
                        <locus>(fol. 1)</locus>
                        <author key="person_88034428">Maximianus</author>
                        <title key="work_3155">Elegiae</title>
                     </msItem>
                     <msItem xml:id="MS_Bodl_38-part1-item2" n="2" class="#versus">
                        <locus>(fol. 13)</locus>
                        <title type="desc" key="work_13886">Poems</title>
                        <msItem>
                           <incipit>Premia tot forme</incipit>
                           <note>Twenty elegiac lines.</note>
                        </msItem>
                        <msItem>
                           <incipit>Lux oculis lux bbanda</incipit>
                           <note>Eighteen elegiac lines.</note>
                        </msItem>
                        <msItem>
                           <incipit>Quisquis ad excelsi</incipit>
                           <note>Twenty-three hexameters.</note>
                        </msItem>
                        <msItem>
                           <incipit>Quod micat ornatum</incipit>
                           <note>Twenty-three elegiac lines, vv. 2-36 on a later leaf in the hand of Petrus Daniel, who adds collations of the other poems with a different manuscript.</note>
                        </msItem>
                     </msItem>
                  </msContents>
