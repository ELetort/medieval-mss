                  <altIdentifier type="internal">
                     <idno type="SCN">Not in SC (late accession)</idno>
                  </altIdentifier>
               </msIdentifier><head>Prayers and devotional treatises in French; France, 15th century, first half</head>
               <msContents>
                  
                  <textLang mainLang="fr">French</textLang>
                  <msItem xml:id="MS_Holkham_misc_45-item1" n="1">
                     <locus>(fols. 1r-)</locus> 
                     <rubric>Ceste oroison doit on dire
				  quant on se lieve</rubric> 
                     <incipit>Tres haut empereres a cui toute creature</incipit> 
                     <note>(Sonet, 
				  <title>Repertoire</title>, no. 2226).</note>  
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item2" n="2">
                     <locus>(fols. 3r-)</locus> 
                     <rubric>Ceste oroison doit on dire
				  chascun jour au matin</rubric> 
                     <incipit>Tres debonaires peirs tres puissans</incipit> 
                     <note>(Sonet,
				  no. 2129)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item3" n="3">
                     <locus>(fol. 4v)</locus> 
                     <rubric>Ceste oroison doit on dire
				  tous les jours a la messe</rubric> 
                     <incipit>Sire dous Ihesu Crist tu es li souverains
				  evesques</incipit>  
                     <note>(Sonet, no. 2024)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item4" n="4">
                     <locus>(fols. 8v-)</locus> 
                     <rubric>Ceste oroison doit on dire
				  tous les iours apres la levacion</rubric> 
                     <incipit>Tres debonaires et tres douz diex I.C.
				  qui par votre grant douceur</incipit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item5" n="5">
                     <locus>(fols. 9v)</locus> 
                     <rubric>Ceste oroison doit on dire
				  tous les jours apres <gap extent="3" unit="chars" reason="illegible"/>ier et en lieu de graces</rubric> 
                     <incipit>Tres beneures Diex
				  qui a toute creature donnez soustenance</incipit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item6" n="6">
                     <locus>(fols. 11v-)</locus> 
                     <rubric>Ceste oroison doit on
				  dire ... a vespres</rubric> 
                     <incipit>Haut roys pardurables I.C. filz Dieu le pere</incipit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item7" n="7">
                     <locus>(fols. 13v-)</locus> 
                     <rubric>Ceste oroison doit on
                        dire a vespres</rubric> 
                     <incipit>Sire dous I.C. soulans de joustice</incipit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item8" n="8">
                     <locus>(fols. 14r-)</locus> 
                     <rubric>Ceste oroison doit on
				  dire ... avant coucher</rubric> 
                     <incipit>Dous peres des ciex qui par nature</incipit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item9" n="9">
                     <locus>(fols. 16v-)</locus> 
                     <rubric>Ceste oroison doit on
				  dire ... avant coucher</rubric> 
                     <incipit>Tres dous sires pardonnes moi tous mes mesfais</incipit>
				  
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item10" n="10">
                     <locus>(fols. 17v-)</locus> 
                     <rubric>Ceste oroison doit on
				  dire quant l'en se doit confesser ...</rubric> 
                     <incipit>Nostre tres dous pers des ciex de
				  la sus</incipit>  
                     <note>(Sonet, no. 1265)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item11" n="11">
                     <locus>(fols. 21r-)</locus> 
                     <rubric>A celui haut seignier qui
				  ainsi fet amer sans aurte meeneire (?) deves parler avec sant Augustin</rubric>
				                 <incipit>Souverains roys I.C. tous puissans tres debonnaires</incipit>  
                     <note>(Sonet, no.
				  2090)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item12" n="12">
                     <locus>(fols. 23r-)</locus> 
                     <rubric>Ceste oroison fist misres
				  saint Thomas ...</rubric> 
                     <incipit>Pardurables gouvernierres sire</incipit> 
                     <note>(Sonet, no.
				  1661)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item13" n="13">
                     <locus>(fols. 23v-)</locus> 
                     <rubric>Ceste oroison si est por
				  prier qui mal nous fet</rubric> 
                     <incipit>Sauvierres I.C. tres debonaires sires</incipit> 
				                 <note>(Sonet, no. 1884)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item14" n="14">
                     <locus>(fols. 23v-)</locus> 
                     <rubric>Ceste oroison est por
				  prier Dieu et Nostre Dame et saint Iohan l'evangeliste</rubric> 
                     <incipit>E tres enterine</incipit>
				                 <note>(a translation of the O intemerata, Sonet, no. 568)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item15" n="15">
                     <locus>(fols. 24v-)</locus> 
                     <rubric>Ceste oroison si est pour
				  le corps Ihesu Crist ...</rubric> 
                     <incipit>La pais de Dieu, la face de notre seingnieur</incipit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item16" n="16">
                     <locus>(fols. 25r-)</locus> 
                     <incipit>Sauveres I.C. ie pecheresse</incipit>  
                     <note>(Sonet, no.
				  1885)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item17" n="17">
                     <locus>(fols. 25v-)</locus> 
                     <incipit>Virge Marie mere de I.C. en tes mains</incipit> 
				                 <note>(Sonet, no. 2325)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item18" n="18">
                    <locus>(fols. 25v-)</locus> 
                     <incipit>Sainte Marie virge mere Dieu tres piteuse</incipit> 
				                 <note>(Sonet, no. 1870)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item19" n="19">
                     
                     <locus>(fols. 27v-)</locus> 
                     <incipit>Ie te prie sainte Marie dame royne du ciel</incipit> 
				                 <note>(Sonet, no. 852)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item20" n="20">
                     <locus>(fols. 28v-)</locus> 
                     <author key="person_7011404">Ps.-Bonaventure (James of Milan)</author>
                     <title key="work_3969">Stimulus amoris</title>
                     <note>(ch. 1, French translation)</note>
                     <rubric>Ceste livre est apele
				  l'aguillon de amer le tres doulz et le tres debonneure Ihesu</rubric> 
                     <incipit>Courez
				  hommes et fames et toutes gens</incipit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item21" n="21">
                   
                     <locus>(fols. 34v-)</locus> 
                     <rubric>Ceste oroison que sensuit
				  tu la dois dire tours jours ...</rubric> 
                     <incipit>Sire saint pere otirie a moy pecheur</incipit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item22" n="22">
                     
                     <locus>(fols. 34v-)</locus> 
                     <rubric>Pour soi excitera avoir
				  pitis de messieur Ihesu Crist</rubric> 
                     <incipit>A auoir compassion de nostre seigneur Dieu
				  I.C.</incipit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item23" n="23">
                     
                     <locus>(fols. 41r-)</locus> 
                     <rubric>Comment l'en doit penser
				  la doleur et la pitie que la mere Dieu et a la passion son filz</rubric> 
                     <incipit>Iouste
				  la crois Ihesu estoit Marie</incipit>  
                     <explicit>en luminement en delices ... per omnia secula
				  seculorum. Amen</explicit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item24" n="24">
                     <locus>(fols. 54v-)</locus> 
                     <rubric>Comment home sera fervent
				  de fere les cures qui plaisent a Dieu</rubric> 
                     <incipit>O home se tu regardoises que Dieu
				  te fist</incipit> 
                     <explicit>qui sont dignes de mort</explicit>
                    
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item25" n="25">
                     
                     <locus>(fols. 65r-)</locus> 
                     <rubric>Comment on doit estre
				  ordene a son prochain</rubric> 
                     <incipit>De ton prochein prey et recoif breve dotrine</incipit> 
				                 <explicit>ne tende a autre chose</explicit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item26" n="26">
                    <locus>(fols. 68v-)</locus> 
                     <title key="work_13260">Meditation</title>
                     <incipit>Quicunques veust a la hautesse
				  nostre seigneur</incipit>
                    
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item27" n="27">
                     
                     <locus>(fols. 70r-)</locus> 
                     <rubric>Comment o grant diligente
				  lespouse doit garder son ami</rubric> 
                     <incipit>O la hautesse des richesses de la sapience</incipit> 
                     <note>ending imperfect due to the loss of a leaf after fol. 71</note>
                     
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item28" n="28">
                    <locus>(fols. 72r-)</locus>
                     <author key="person_306415749">Ps.-Bernard</author>
                     <title key="work_3939">Planctus Mariae</title>
                     <note>(free French translation; the Latin text is pr. in 
				  D. Caillau, 
					 <title>Appendix serm. ined. S. Augustini</title> (Paris,
					 1842), pp. 238-41.</note>
                     <incipit>Ha, dist saint Bernart, qui donra a mon chief
				  eaue et a mes yeux fontaine de larmes</incipit>  
                     <explicit>sans fin avec vostre doulz filz</explicit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item29" n="29">
                     <locus>(fols. 85r-)</locus> 
                     <rubric>Cy sensuit comment on
				  doit cir la messe devotement</rubric> 
                     <incipit>A lentree de la messe doit chascun homme et
				  chascune femme restraindre ses pensees</incipit>  
                     <explicit>quil ne nous oublie unie</explicit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item30" n="30">
                     <locus>(fols. 89r-)</locus> 
                     <rubric>Devote oroison de
				  monseigneur s. Michiel</rubric> 
                     <incipit>Tres glorieux archange prince</incipit>  
                     <note>(Sonet, no.
				  2222)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item31" n="31">
                     
                     <locus>(fols. 90v-)</locus> 
                     <rubric>Cy sensuit une tres noble
				  et excellente oroison a nostre seigneur Ihesu Crist</rubric> 
                     <incipit>Mon tres puissant
				  createur</incipit> 
                     <note>(Sonet, no. 1227)</note>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item32" n="32">
                    <locus>(fols. 92r-)</locus> 
                     <title key="work_10421">Athanasian Creed in French</title> 
                     <incipit>Quiconques veult
				  estre sauf</incipit> 
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item33" n="33">
                     <locus>(fols. 94v-)</locus> 
                     <rubric>Ceste le recors du sermon
				  que maistre Aubers arcevesque de Couloingne fist a toutes bonnes creatures on
				  quel il dist IX parolles prouffitables pour le prouffit de lame</rubric> 
                     <incipit>La
				  premiere si est que creature</incipit> 
                     <explicit>donnast tout le monde pour Dieu</explicit>
                  </msItem>
                  <msItem xml:id="MS_Holkham_misc_45-item34" n="34">
                     
                     
                        <locus>(fols. 96r-)</locus>
                     <rubric>Cy apres sensuit
				  lordonnance de trouver les jeunes grans jeunes de lan et les advens de
				  Noel</rubric>
                     <incipit defective="true">Apres la tiephaigne queres prime lune</incipit> 
                     <note>Ending imperfect due to
				  the loss of leaves at the end of the manuscript.</note>
                  </msItem>
               </msContents>
